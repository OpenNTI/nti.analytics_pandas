<metal:block
	xmlns:tal="http://xml.zope.org/namespaces/tal"
	xmlns:metal="http://xml.zope.org/namespaces/metal"
	tal:define="options view/options">

	<tal:block tal:condition="python: options['has_course_catalog_view_data']">
		<para style="normal" spaceAfter=".5in" >
			This document presents an overview of enrollment related events in <u>${options/course_names}</u> from <strong>${context/start_date}</strong> through <strong>${context/end_date}</strong>.
		</para>
	</tal:block>	
	<hr width="100%" />

	<setNextTemplate name="main"/>
	<nextPage />
	<tal:block tal:condition="python: options['has_course_catalog_view_data']">
		<h1 style="h1" fontSize="14" spaceBefore=".3in" spaceAfter=".3in">
			Overview of Course Catalog Views
		</h1>
		<hr width="100%" />
		<tal:block tal:define="course_catalog_views options/data/course_catalog_views">
			<illustration width="5in" height="3in"
					  	  tal:define="event course_catalog_views/event_course_catalog_views;
					  	  			  user_event course_catalog_views/ratio_course_catalog_views;
								      ratio_event course_catalog_views/user_course_catalog_views;
								      additional_plot course_catalog_views/average_time_length"
					      metal:use-macro="macro:four_plots_event">
			</illustration>
		</tal:block>

		<setNextTemplate name="main"/>
		<nextPage />
		<tal:block tal:condition="python: options['has_course_catalog_views_per_device_types']">
			<h1 style="h1" fontSize="14" spaceBefore=".3in" spaceAfter=".3in">
				Overview of Course Catalog Views
			</h1>
			<hr width="100%" />
			<tal:block tal:define="course_catalog_views options/data/course_catalog_views_per_device_types">
				<illustration width="5in" height="3in"
						  	  tal:define="event course_catalog_views/event_course_catalog_views_per_device_types;
						  	  			  user_event course_catalog_views/ratio_course_catalog_views_per_device_types;
									      ratio_event course_catalog_views/user_course_catalog_views_per_device_types;
									      additional_plot course_catalog_views/average_time_length_per_device_types"
						      metal:use-macro="macro:four_plots_event">
				</illustration>
			</tal:block>
		</tal:block>
	</tal:block>

</metal:block>