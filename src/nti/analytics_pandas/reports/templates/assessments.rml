							  tal:define="event assignment_taken/event_assignments_taken_per_device_types;
										  user_event assignment_taken/user_assignments_taken_per_device_types;
										  ratio_event assignment_taken/ratio_assignments_taken_per_device_types"
							  metal:use-macro="macro:three_plots_event">
				</illustration>
