		</tal:block>

		<setNextTemplate name="main"/>
		<nextPage />
		<tal:block tal:condition="python: options['has_assignment_taken_per_course_sections']">
			<h1 style="h1" fontSize="14" spaceBefore=".3in" spaceAfter=".3in">
				Assignments Taken per Course Sections
			</h1>
			<hr width="100%" />
			<tal:block tal:define="assignment_taken options/data/assignment_taken_per_course_sections/all_section_plots">
				<illustration width="5in" height="3in"
							  tal:define="event assignment_taken/event_assignments_taken_per_course_sections;
										  user_event assignment_taken/user_assignments_taken_per_course_sections;
										  ratio_event assignment_taken/ratio_assignments_taken_per_course_sections"
							  metal:use-macro="macro:three_plots_event">
				</illustration>
			</tal:block>
		</tal:block>
	</tal:block>

	<setNextTemplate name="main"/>
	<nextPage />
	<tal:block tal:condition="python: options['has_self_assessment_views_data']">
		<h1 style="h1" fontSize="14" spaceBefore=".3in" spaceAfter=".3in">
			Overview of Self Assessments Views
		</h1>
